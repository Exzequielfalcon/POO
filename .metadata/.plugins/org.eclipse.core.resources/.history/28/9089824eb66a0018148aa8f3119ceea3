import java.io.BufferedReader;
import java.io.InputStreamReader;


public class Main {
 final static int MAXC=10;
 final static int MAXmov=10;
	public static void main(String[] args) {
		Cliente clientes[] = new Cliente[MAXC];
		clientes[0] = new Cliente("juan","gonzales",123123,true);
		clientes[1] =  new Cliente("mauro","kio",234,true);
		clientes[2] = new Cliente("Matias","Manca",456,true);
		clientes[3] = new Cliente("Fran","Elite",890,true);
		clientes[4] = new Cliente("roberto", "rltecla",6431,true);
		
		Movimiento mov[] = new Movimiento [10];
		boolean encontrado= true;
		Cuenta cuentas[]= new Cuenta[MAXC];
		cuentas[0] = new Cuenta(0,clientes[0], mov);
		cuentas[0].setMonto(5000);
		cuentas[1] = new Cuenta(0,clientes[1], mov);
		Movimiento nuevo =  new Movimiento(100, 2, "retiro");
		Cuenta.AgregarMov(nuevo, cuentas, 0);
		Cuenta.AgregarMov(nuevo, cuentas, 1);
		Cuenta.AgregarMov(nuevo, cuentas, 1);
		System.out.println(cuentas[0].getMov()[0].getMonto());
		System.out.println(cuentas[0].getMov()[0].getTipo());
		System.out.println(cuentas[0].getMov()[0].getOperacion());
