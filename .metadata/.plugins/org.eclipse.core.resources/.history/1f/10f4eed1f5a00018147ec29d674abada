public class ddr6 {
	final static int MAX = 3;

	public static void RandomNoRepeat(int b[][]) {
		boolean c = true;
		for (int i = 0; i < MAX; i++) {
			for (int j = 0; j < MAX; j++) {
				b[i][j] = (int) (Math.random() * 100);

			}
		}
	}

	public static boolean Check(int b[][], int numero) {
		boolean c = true;
		for (int i = 0; i < MAX; i++) {
			for (int j = 0; j < MAX; j++) {
				if (numero == b[i][j]) {
					c = false;
				}
			}
		}
		return c;
	}

}